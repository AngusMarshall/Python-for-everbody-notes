name = input("Enter file:")
if len(name) < 1:
    name = "mbox-short.txt"
handle = open(name)

count = dict()

for line in handle:
    if line.startswith("From:"):
        words = line.split()
        time = words[:5]
        hour = time[:2]
        count[hour] = count.get(hour,0) + 1
        
for key, val in sorted(count.items()):
    print(key,val)
